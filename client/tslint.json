{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "interface-name": false,
      "member-ordering": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "ban-types": false
    },
    "linterOptions": {
      "exclude": [
        "./node_modules/**"
      ],
      "include": ["src"]
    }
}